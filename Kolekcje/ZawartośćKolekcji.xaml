<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Kolekcje.ZawartoscKolekcji">

    <!-- Zawartość strony -->
    <StackLayout>
        <Label x:Name="Label" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand" />
        <CollectionView x:Name="mycollectionView" />
        <Button Text="Nowy Element" Clicked="NowyElement_Clicked" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand"/>
    </StackLayout>

</ContentPage>
